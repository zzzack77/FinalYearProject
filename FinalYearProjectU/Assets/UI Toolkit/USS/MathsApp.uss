    background-image: url("project://database/Assets/Dark%20UI/New%20Icons/White%20Backward%202.png?fileID=2800000&guid=f30c643a213a8164f96258d141ea1107&type=3#White Backward 2");
    -unity-background-image-tint-color: rgb(0, 0, 0);
